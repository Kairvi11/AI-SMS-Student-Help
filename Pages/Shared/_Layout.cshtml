            <div class="container">
                <div class="navbar-collapse collapse">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <div class="logo-container">
                                <a class="navbar-brand navbar-brand-custom" asp-area="" asp-page="/Index">Faculty Portal</a>
                            </div>
                        </li>
                    </ul>
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                            aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                        <ul class="navbar-nav flex-grow-1 home-register-links">
                            <li class="nav-item">
                                <a class="nav-link text-light" asp-area="" asp-page="/Index">Login</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-light" asp-area="" asp-page="/Privacy">Register</a>
                            </li>
                            <li class="nav-item">
<<<<<<< Updated upstream
                                <a class="nav-link text-light" asp-area="" asp-page="/EditProfil">Edit Profil</a>
=======
                                <a class="nav-link text-light" asp-area="" asp-page="/EditProfile">Edit Profile</a>
>>>>>>> Stashed changes
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>